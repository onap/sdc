        <dependency>
            <groupId>org.openecomp.sdc.onboarding</groupId>
            <artifactId>vendor-software-products-rest-services</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.onap.sdc.common</groupId>

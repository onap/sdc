{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "lib": [
      "es6",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
     ]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../node_modules/@types",
      "./typings"
    ],
    "types": [
      "core-js",
      "node",
      "jest"
    ],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
  ]
}
        }
      },
      "TopicRegistrationResponse" : {
        "type" : "object",
        "properties" : {
          "distrNotificationTopicName" : {
            "type" : "string"
          },
          "distrStatusTopicName" : {
            "type" : "string"
          }
        }
      },
      "TopicUnregistrationResponse" : {
        "type" : "object",
        "properties" : {
          "distrNotificationTopicName" : {

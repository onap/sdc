  "components" : {
    "schemas" : {
      "RegistrationRequest" : {
        "type" : "object",
        "properties" : {
          "apiPublicKey" : {
            "type" : "string"
          },
          "distEnvEndPoints" : {
            "type" : "array",
            "items" : {
              "type" : "string"
            }
          },
          "distrEnvName" : {
            "type" : "string"
          },
